package Ability;

public class ProjectileSector 
{
	private double angle;
	
	private int pivots;
	
	public ProjectileSector()
	{
		angle = 0;
		pivots = 0;
	}
	
	public ProjectileSector(double angle, int pivots)
	{
		this.angle = angle;
		this.pivots = pivots;
	}
	
	public void setAngle(double angle) {
		this.angle = angle;
	}
	
	public void setPivots(int pivots) {
		this.pivots = pivots;
	}
	
	public double getAngle() {
		return angle;
	}
	
	public int getPivots() {
		return pivots;
	}
}
