package Editor;

public class SnapSettings 
{
	private double width, height;
	
	public SnapSettings()
	{
		width = height = 20;
	}
	
	public void setTileWidth(double width) {
		this.width = width;
	}
	
	public void setTileHeight(double height) {
		this.height = height;
	}
	
	public double getTileWidth() {
		return width;
	}
	
	public double getTileHeight() {
		return height;
	}
}
