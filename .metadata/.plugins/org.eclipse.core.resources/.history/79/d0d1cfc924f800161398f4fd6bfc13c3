package Editor.comp;

import javax.swing.JLabel;

import Editor.DoubleField;

public class VectorForm extends Form 
{
	private DoubleField xFld, yFld;
	
	public VectorForm()
	{
		JLabel xLbl = new JLabel("X"),
			   yLbl = new JLabel("Y");
		
		xFld = new DoubleField();
		yFld = new DoubleField();
	}
}
