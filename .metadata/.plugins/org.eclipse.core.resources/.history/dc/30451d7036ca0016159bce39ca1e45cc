package Tileset;

public class TMCell 
{
	public final TileMap src;
	
	public final int row, col;
	
	public TMCell(TileMap src, int row, int col)
	{
		this.src = src;
		
		this.row = row;	this.col = col;
	}
	
	public void set(Tile tile)
	{
		
	}
	
	public Tile get()
	{
		return src.get(row, col);
	}
}
