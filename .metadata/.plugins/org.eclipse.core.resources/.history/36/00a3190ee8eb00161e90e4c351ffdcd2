package Quest;

import java.util.ArrayList;
import java.util.List;

import Item.ConsumableItem;
import Item.EquipItem;

public class Inventory 
{
	private List<EquipItem> equips;
	
	private List<ConsumableItem> consumables;
	
	public Inventory()
	{
		equips = new ArrayList<EquipItem>();
		
		consumables = new ArrayList<ConsumableItem>();
	}
	
	public void addEquipItem(EquipItem equip) {
		equips.add(equip);
	}
	
	public void addConsumblableItem(ConsumableItem consumable) {
		consumables.add(consumable);
	}
	
	public void add
}
