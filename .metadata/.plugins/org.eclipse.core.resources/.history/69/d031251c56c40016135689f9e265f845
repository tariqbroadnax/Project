package Tileset;

import java.util.HashMap;
import java.util.Map;

public class LazyMultiArray<E>
{
	private Map<Index, E[][]> map;
	
	public LazyMultiArray(int blockWidth, int blockHeight)
	{
		map = new HashMap<Index, E[][]>();
	}
	
	public void set(int row, int col)
	{
		
	}
	
	private class Index
	{
		public int startRow, startCol;
		
		
	}
}
