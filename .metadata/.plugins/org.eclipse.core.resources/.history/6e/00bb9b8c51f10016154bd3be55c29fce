package Editor;

public class SnapSettings 
{
	private double width, height;
	
	public SnapSettings()
	{
		width = height = 20;
	}
	
	public void setSnapWidth(double width) {
		this.width = width;
	}
	
	public void setSnapHeight(double height) {
		this.height = height;
	}
	
	public double getSnapWidth() {
		return width;
	}
	
	public double getSnapHeight() {
		return height;
	}
}
