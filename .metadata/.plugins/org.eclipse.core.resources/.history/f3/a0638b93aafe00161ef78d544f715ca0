package Quest;

import Dialogue.Dialogue;

public class DialogueTask extends Task
{
	private Dialogue d;
	
	public DialogueTask(){}
	
	public DialogueTask(Dialogue d)
	{
		this.d = d;
	}
	
	public DialogueTask(DialogueTask task)
	{
		d = task.d;
	}

	@Override
	public void start() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public boolean isFinished() {
		// TODO Auto-generated method stub
		return false;
	}

	@Override
	public Object clone() {
		// TODO Auto-generated method stub
		return null;
	}
}
