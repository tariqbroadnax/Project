package Editor.comp;

import javax.swing.JLabel;
import javax.swing.JPanel;
import javax.swing.JTextField;
import javax.swing.JToggleButton;

public class MovementEditor extends JPanel
{
	private JLabel speedLbl, dirLbl,
				   enabledLbl;
	
	private JTextField speedFld, dirFld;
	
	private JToggleButton enabledBtn;
	
	public MovementEditor()
	{
		speedLbl = new JLabel("Speed");
		dirLbl = new JLabel("Direction");
		enabledLbl = new JLabel("Enabled");
		
		speedFld = new JTextField();
		dirFld = new JTextField();
		
		enabledBtn = new JToggleButton();
	}
}
